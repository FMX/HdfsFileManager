<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="118.0" prefWidth="353.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="NewConnectionDialog">
   <children>
      <VBox prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox alignment="CENTER_LEFT" prefHeight="27.0" prefWidth="353.0">
               <children>
                  <Label text="HDFS地址">
                     <HBox.margin>
                        <Insets left="10.0" right="10.0" />
                     </HBox.margin></Label>
                  <TextField fx:id="hdfs_address_textfield" HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin></TextField>
               </children>
            </HBox>
            <HBox alignment="CENTER_LEFT" prefHeight="36.0" prefWidth="353.0">
               <children>
                  <Label text="HDFS端口">
                     <HBox.margin>
                        <Insets left="10.0" right="10.0" />
                     </HBox.margin></Label>
                  <TextField fx:id="hdsf_port_textfield" HBox.hgrow="ALWAYS" />
               </children>
            </HBox>
            <HBox alignment="CENTER_LEFT" prefHeight="28.0" prefWidth="353.0">
               <children>
                  <Label text="HDFS用户">
                     <HBox.margin>
                        <Insets left="10.0" right="10.0" />
                     </HBox.margin></Label>
                  <TextField fx:id="hdfs_root_textfield" HBox.hgrow="ALWAYS" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="44.0" prefWidth="353.0">
               <children>
                  <Button fx:id="test_connect" mnemonicParsing="false" prefHeight="27.0" prefWidth="75.0" text="测试连接">
                     <HBox.margin>
                        <Insets right="30.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="connect" mnemonicParsing="false" prefHeight="27.0" prefWidth="75.0" text="连接" />
                  <Button fx:id="cancel_connect" mnemonicParsing="false" prefHeight="27.0" prefWidth="75.0" text="取消">
                     <HBox.margin>
                        <Insets left="30.0" />
                     </HBox.margin>
                  </Button>
               </children>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
